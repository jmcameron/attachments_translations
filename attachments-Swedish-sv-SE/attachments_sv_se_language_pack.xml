<?xml version="1.0" encoding="utf-8"?>
<extension version="2.5" type="file" method="upgrade">
  <name>attachments_sv_se_language_pack</name>
  <version>3.1-Beta3</version>
  <creationDate>2013-05-18</creationDate>
  <author>Jonathan M. Cameron, Jonas Hedberg (3.0), Marcus Ludvigson (3.0), Linda Maltanski (2.2)</author>
  <authorEmail>jmcameron@jmcameron.net</authorEmail>
  <authorUrl></authorUrl>
  <copyright>Copyright (C) 2013 Jonathan M. Cameron. All rights reserved.</copyright>
  <license>GNU GPL 3: http://www.gnu.org/licenses/gpl-3.0.html</license>
  <description>ATTACHMENTS_SV_SE_LANGUAGE_PACK</description>
  <scriptfile>install.php</scriptfile>
  <fileset>

    <!-- Admin component language folder -->
    <files folder="attachments_component/admin" target="administrator/components/com_attachments/language/sv-SE">
      <filename>index.html</filename>
      <filename>sv-SE.com_attachments.ini</filename>
      <filename>sv-SE.com_attachments.sys.ini</filename>
      <filename>sv-SE.com_attachments.help.ini</filename>
    </files>

    <!-- Admin component sv-SE language help images folder -->
    <files folder="attachments_component/media/images/help" target="media/com_attachments/images/help">
      <folder>sv-SE</folder>
    </files>

    <!-- Site component language folder -->
    <files folder="attachments_component/site" target="components/com_attachments/language/sv-SE">
      <filename>index.html</filename>
      <filename>sv-SE.com_attachments.ini</filename>
    </files>

    <!-- Attachments plugin language files -->
    <files folder="attachments_plugin" target="plugins/content/attachments/language/sv-SE">
      <filename>index.html</filename>
      <filename>sv-SE.plg_content_attachments.ini</filename>
      <filename>sv-SE.plg_content_attachments.sys.ini</filename>
    </files>

    <!-- Attachments search plugin language files -->
    <files folder="attachments_search" target="plugins/search/attachments/language/sv-SE">
      <filename>index.html</filename>
      <filename>sv-SE.plg_search_attachments.ini</filename>
      <filename>sv-SE.plg_search_attachments.sys.ini</filename>
    </files>

    <!-- Attachments add attachment editor button language files -->
    <files folder="add_attachment_btn_plugin" target="plugins/editors-xtd/add_attachment/language/sv-SE">
      <filename>index.html</filename>
      <filename>sv-SE.plg_editors-xtd_add_attachment.ini</filename>
      <filename>sv-SE.plg_editors-xtd_add_attachment.sys.ini</filename>
    </files>

    <!-- Attachments insert attachment token editor button language files -->
    <files folder="insert_attachments_token_btn_plugin" target="plugins/editors-xtd/insert_attachments_token/language/sv-SE">
      <filename>index.html</filename>
      <filename>sv-SE.plg_editors-xtd_insert_attachments_token.ini</filename>
      <filename>sv-SE.plg_editors-xtd_insert_attachments_token.sys.ini</filename>
   </files>

    <!-- Attachments show attachment in editor plugin language files -->
    <files folder="show_attachments_in_editor_plugin" target="plugins/system/show_attachments/language/sv-SE">
      <filename>index.html</filename>
      <filename>sv-SE.plg_system_show_attachments.ini</filename>
      <filename>sv-SE.plg_system_show_attachments.sys.ini</filename>
    </files>

    <!-- Attachments attachments for content plugin language files -->
    <files folder="attachments_for_content" target="plugins/attachments/attachments_for_content/language/sv-SE">
      <filename>index.html</filename>
      <filename>sv-SE.plg_attachments_attachments_for_content.ini</filename>
      <filename>sv-SE.plg_attachments_attachments_for_content.sys.ini</filename>
    </files>

    <!-- Attachments attachments plugin framework language files -->
    <files folder="attachments_plugin_framework" target="plugins/attachments/attachments_plugin_framework/language/sv-SE">
      <filename>index.html</filename>
      <filename>sv-SE.plg_attachments_attachments_plugin_framework.ini</filename>
      <filename>sv-SE.plg_attachments_attachments_plugin_framework.sys.ini</filename>
    </files>

    <!-- Languages for this set of files -->
    <files folder="language/en-GB" target="language/en-GB">
      <filename>en-GB.files_attachments_sv_se_language_pack.sys.ini</filename>
    </files>
    <files folder="language/sv-SE" target="language/sv-SE">
      <filename>sv-SE.files_attachments_sv_se_language_pack.sys.ini</filename>
    </files>

  </fileset>

  <updateservers>
       <server type="extension" priority="1" name="Attachments Swedish (sv-SE) Language Pack Updates">http://jmcameron.net/attachments/updates/translations/Swedish-sv-SE/updates.xml</server>
  </updateservers>

</extension>

